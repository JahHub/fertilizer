<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity
            name="JahHub\FertilizerBundle\Entity\Week"
            table="week"
            repository-class="JahHub\FertilizerBundle\Repository\WeekRepository"
        >
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <many-to-one target-entity="JahHub\FertilizerBundle\Entity\State" field="state" inversed-by="weekList">
            <join-column name="state_id" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
        </many-to-one>
    </entity>
</doctrine-mapping>
