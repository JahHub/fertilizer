<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="jahhub_fertilizer.form.type.item.class">JahHub\FertilizerBundle\Form\Type\ItemType</parameter>
        <parameter key="jahhub_fertilizer.form.type.state.class">JahHub\FertilizerBundle\Form\StateType</parameter>
    </parameters>

    <services>
        <service id="jahhub_fertilizer.type.item" class="%jahhub_fertilizer.form.type.item.class%">
            <tag name="form.type" alias="fertilizer_item" />
        </service>
        <service id="jahhub_fertilizer.form.type.state" class="%jahhub_fertilizer.form.type.state.class%">
            <tag name="form.type" alias="fertilizer_state" />
        </service>
    </services>
</container>

